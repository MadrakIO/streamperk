{
    "name": "madrakio/streamperk",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "liip/monitor-bundle": "~2.3",
        "madrakio/streamperk-core-bundle": "dev-master",
        "madrakio/streamperk-user-bundle": "dev-master",
        "madrakio/streamperk-server-bundle": "dev-master",
        "madrakio/streamperk-landing-bundle": "dev-master",
        "madrakio/streamperk-poll-bundle": "dev-master",
        "madrakio/streamperk-schedule-bundle": "dev-master",
        "madrakio/streamperk-forum-bundle": "dev-master",
        "madrakio/streamperk-blog-bundle": "dev-master",
        "madrakio/streamperk-giveaway-bundle": "dev-master",
        "madrakio/streamperk-widget-page-bundle": "dev-master",
        "madrakio/streamperk-file-bundle": "dev-master",
        "madrakio/streamperk-tip-bundle": "dev-master",
        "hwi/oauth-bundle": "dev-master",
        "nelmio/cors-bundle": "1.4.*",
        "friendsofsymfony/jsrouting-bundle": "~1.6",
        "madrakio/extendable-configuration-bundle": "dev-master",
        "madrakio/extendable-permissions-bundle": "dev-master"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7",
        "hautelook/alice-bundle": "^1.1",
        "doctrine/data-fixtures": "^1.1"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "5.6.17"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "keep-outdated": true
        }
    }
}
